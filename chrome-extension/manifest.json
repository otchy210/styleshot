{
  "name": "Styleshot",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Google Chrome extension which allows us to take a snapshot of all HTML and CSS.",
  "permissions": [
    "https://styleshot.appspot.com/",
    "storage",
    "webRequest"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },
  "content_scripts": [],
  "browser_action": {
      "default_title": "JIRA Everywhere",
      "default_icon": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "32": "icons/icon32.png"
      },
      "default_popup": "dist/popup.html"
  },
  "options_page": "dist/options.html"
}
